<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CometServlet</servlet-name>
  	<display-name>CometServlet</display-name>
  	<description></description>
  	<servlet-class>chabernac.comet.CometServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>P2PServlet</servlet-name>
  	<display-name>P2PServlet</display-name>
  	<description></description>
  	<servlet-class>chabernac.p2p.P2PServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CometServlet</servlet-name>
  	<url-pattern>/CometServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>P2PServlet</servlet-name>
  	<url-pattern>/P2PServlet</url-pattern>
  </servlet-mapping>
</web-app>
